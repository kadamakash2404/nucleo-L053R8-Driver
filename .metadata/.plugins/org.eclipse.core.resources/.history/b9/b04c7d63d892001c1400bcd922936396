#ifndef INC_STM32L053XX_H_
#define INC_STM32L053XX_H_


#define FLASH_BASEADDR					0x0800 0000U		/* flash memory is up to 192 Kbyts*/
#define SRAM 							0X2000 2000U
#define ROM								0x1FF0 0000U

